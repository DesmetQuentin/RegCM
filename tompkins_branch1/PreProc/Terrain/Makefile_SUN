#  Makefile for RegCM@  run on SUN workstation

FFLAGS = -L../../Commons/env/libsun/ -lnetcdf
FC = f90 -fns -ftrap=common

LD = $(FC)

SRC = terrain.f

OBJ = terrain.o

all: terrain

.f.o:
	$(FC) -c $(FFLAGS) $*.f

terrain: $(OBJ)
	$(LD) -o $@ $(OBJ) $(FFLAGS)

clean:
	rm -f *.o terrain
# --------------------------------------------------------------------
# DO NOT DELETE THIS LINE -- make depend depends on it.

terrain.o: domain.param terrain.f 
